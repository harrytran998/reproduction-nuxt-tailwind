{
  "name": "super-distribution-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "generate": "nuxt generate",
    "setup": "yarn generate",
    "start": "nuxt start",
    "generate:props": "vue-int --output '.vscode/vetur' --input",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js"
  },
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/apollo": "^4.0.1-rc.5",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/composition-api": "^0.16.4",
    "@nuxtjs/svg": "^0.1.12",
    "@vue/apollo-composable": "4.0.0-alpha.10",
    "core-js": "^3.6.5",
    "dayjs": "^1.9.6",
    "element-ui": "^2.14.1",
    "graphql-tag": "^2.11.0",
    "lodash.get": "^4.4.2",
    "lodash.map": "^4.6.0",
    "node-sass": "^5.0.0",
    "nuxt": "^2.14.7",
    "sass-loader": "^10.1.0",
    "vue-agile": "^1.1.3",
    "vue-lazyload": "^1.3.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/pwa": "^3.2.2",
    "@nuxtjs/tailwindcss": "^3.2.0",
    "autoprefixer": "^9",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "postcss": "^7",
    "prettier": "^2.1.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  }
}
